<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item
        android:top="1dp"
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp">
        <rotate
            android:fromDegrees="@integer/jellybean_slant_right_up"
            android:toDegrees="@integer/jellybean_slant_right_up"
            android:pivotX="50%"
            android:pivotY="50%">
            <shape android:shape="oval">
                <solid android:color="#131313"/>
                <stroke
                    android:color="@color/white_50"
                    android:width="1dp"/>
                <size
                    android:height="43dp"
                    android:width="57dp"/>
            </shape>
        </rotate>

    </item>
</layer-list>
