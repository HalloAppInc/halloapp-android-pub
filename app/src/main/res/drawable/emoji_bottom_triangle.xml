<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:bottom="-15dp" android:right="-15dp">
        <rotate
            android:fromDegrees="-45"
            android:toDegrees="0"
            android:pivotX="150%"
            android:pivotY="20%" >
            <shape
                android:shape="rectangle" >
                <solid android:color="@color/emoji_variant_triangle_color" />
            </shape>
        </rotate>
    </item>
</layer-list>
