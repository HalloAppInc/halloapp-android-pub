<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="24dp"
    android:height="24dp"
    android:viewportWidth="24"
    android:viewportHeight="24">
  <item>
    <shape android:shape="oval">
      <size
          android:width="24dp"
          android:height="24dp" />

      <solid
          android:color="@color/black_70" />
    </shape>
  </item>

  <item
      android:drawable="@drawable/ic_save_white"
      android:left="4dp"
      android:right="4dp"
      android:top="4dp"
      android:bottom="4dp"
      android:width="20dp"
      android:height="20dp"
    />
</layer-list>
